{
  "name": "css-mediaquery2",
  "version": "1.0.8",
  "description": "Parses and determines if a given CSS Media Query matches a set of values.",
  "repository": {
    "type": "git",
    "url": "https://github.com/efoken/css-mediaquery2.git"
  },
  "bugs": {
    "url": "https://github.com/efoken/css-mediaquery2/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup src --dts",
    "lint": "eslint src",
    "test": "jest"
  },
  "author": "Eike Foken <eikefoken@gmail.com>",
  "license": "ISC",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "tsup": {
    "minify": true,
    "target": "es2019",
    "format": [
      "cjs",
      "esm"
    ]
  }
}
